class Solution:
    def areaOfMaxDiagonal(self, dimensions: List[List[int]]) -> int:
        n = len(dimensions)
        if n == 1: 
            return dimensions[0][0] * dimensions[0][1]
        temp = 0    
        ans = 0
        ar = 1
        maxarea=1
        for i in range(n):
            a = dimensions[i][0]
            b = dimensions[i][1]
            s = a * a + b * b
            arr = a * b
            if s > ans:
                ans = s
                temp = i
                maxarea=arr
            elif s==ans:
                maxarea=max(maxarea,arr)
        return maxarea
