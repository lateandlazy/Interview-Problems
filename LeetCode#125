class Solution:
    def isPalindrome(self, s: str) -> bool:
        ans = []
        for ch in s:
            if ch.isalnum():
                ans.append(ch.lower())
        left = 0
        right = len(ans) - 1
        while left < right :
            if ans[left] != ans[right]:
                return False
            left += 1
            right -= 1
        return True  
